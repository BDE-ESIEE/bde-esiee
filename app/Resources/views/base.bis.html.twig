<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" type="image/vnd.microsoft.icon" href="{{ asset('favicon.ico') }}" />
        <title>{% block title %}Ferus{% endblock %}</title>

        {% block stylesheets %}
            {% stylesheets 
                'bundles/applicationbde/css/bootstrap.min.css'
                'bundles/applicationbde/css/theme.css'
                'bundles/applicationbde/css/bootstrap-reset.bis.css'
                'bundles/applicationbde/css/font-awesome.css'
                'bundles/applicationbde/css/style.bis.css'
                'bundles/applicationbde/css/style-responsive.bis.css'

                filter='cssrewrite,?yui_css' 
            %}
                <link rel="stylesheet" href="{{ asset_url }}" type='text/css' />
            {% endstylesheets %} 
        {% endblock %}
        <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic" type='text/css' />
        

        <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
        <!--[if lt IE 9]>
          <script src="{{ asset('bundles/applicationbde/js/html5shiv.js') }}"></script>
          <script src="{{ asset('bundles/applicationbde/js/respond.min.js') }}"></script>
        <![endif]-->
    </head>

    <body>
        <!--header start-->
        <header class="header-frontend">
            <div class="navbar navbar-default navbar-static-top">
                <div class="container">
                    <div class="navbar-header">
                        <a class="navbar-brand" href="{{ url('application_bde_homepage') }}"><span>FERUS</span> - BDE ESIEE PARIS</a>
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="fa fa-bars"></span>
                        </button>
                    </div>
                    <div class="navbar-collapse collapse ">
                        <ul class="nav navbar-nav">
                            <li {% block liAccueil %}{% endblock %}>
                                <a href="{{ path('application_bde_homepage') }}">
                                    <i class="fa fa-home"></i>
                                    Accueil
                                </a>
                            </li>
                            <li {% block liNews %}{% endblock %}>
                                <a href="{{ path('sonata_news_archive') }}">
                                    <i class="fa fa-archive"></i>
                                    News
                                </a>
                            </li>
                            <li {% block liClubs %}{% endblock %}>
                                <a href="{{ path('application_bde_club') }}">
                                    <i class="fa fa-rocket"></i>
                                    Associatif
                                </a>
                            </li>
                            <li {% block liCalendrier %}{% endblock %}>
                                <a href="{{ path('application_bde_events') }}">
                                    <i class="fa fa-calendar"></i>
                                    Calendrier
                                </a>
                            </li>
                            <li class="dropdown{% block liServices %}{% endblock %}">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false">
                                    <i class="fa fa-cloud"></i>
                                    Services <i class="fa fa-angle-down"></i>
                                </a>
                                <ul class="dropdown-menu">
                                    <li>
                                        <a href="https://bde.esiee.fr/annales/">
                                            <i class="fa fa-book fa-fw"></i>
                                            Annales
                                        </a>
                                    </li>
                                    <li>
                                        <a href="{{ path('application_bde_eat_traffic') }}">
                                            <i class="fa fa-cutlery fa-fw"></i>
                                            Eat'Traffic
                                        </a>
                                    </li>
                                    <li>
                                        <a href="{{ path('application_bde_breguet_shop') }}">
                                            <i class="fa fa-shopping-cart fa-fw"></i>
                                            Breguet Shop
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="fa fa-lightbulb-o fa-fw"></i>
                                            Bons Plans
                                        </a>
                                    </li>
                                    <li>
                                        <a href="{{ path('application_bde_lost_property') }}">
                                            <i class="fa fa-archive fa-fw"></i>
                                            Objets Trouvés
                                        </a>
                                    </li>
                                </ul>
                            </li>
                            <li {% block liMedia %}{% endblock %}>
                                <a href="{{ path('sonata_media_gallery_index') }}">
                                    <i class="fa fa-camera-retro"></i>
                                    Media
                                </a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false">
                                    <i class="fa fa-user"></i>
                                    <i class="fa fa-angle-down"></i>
                                </a>
                                <ul class="dropdown-menu">
                                    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                        <li><a href="{{ path('sonata_user_profile_show') }}">{{ app.user.username }}</a></li>
                                        <li><a href="{{ path('sonata_user_profile_show') }}"><i class="fa fa-suitcase fa-fw"></i>Profile</a>
                                        </li>
                                        {% if app.security and app.security.token and is_granted('ROLE_SONATA_ADMIN') %}
                                            <li>
                                                <a href="{{ path('sonata_admin_dashboard') }}">
                                                    <i class="fa fa-tachometer fa-fw"></i>{{ "title_dashboard"|trans({}, 'SonataAdminBundle') }}
                                                </a>
                                            </li>
                                        {% endif %}
                                        <li><a href="#"><i class="fa fa-cog fa-fw"></i>Paramètres</a>
                                        </li>
                                        {# <li><a href="#"><i class="fa fa-bell-o fa-fw"></i> Notification</a>
                                        </li> #}
                                        <li><a href="{{ path('sonata_user_security_logout') }}"><i class="fa fa-key fa-fw"></i>Déconnexion</a>
                                        </li>
                                    {% else %}
                                        <li><a href="{{ path('sonata_user_security_login') }}">Connexion</a></li>
                                    {% endif %}
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>
        <!--header end-->

        {% block breadcrumbs %}
        {% endblock %}

        <!--container start-->
        {% block container %}
        <div{% block containerClass %}{% endblock %}>
            <div class="container">
                <div class="row">
                    {% for type, flashMessages in app.session.flashbag.all() %}
                        {% for flashMessage in flashMessages %}
                            <div class="alert alert-{{ type }}">
                                {{ flashMessage }}
                            </div>
                        {% endfor %}
                    {% endfor %}
                    {% block body %}
                    {% endblock %}
                </div>
            </div>
        </div>
        {% endblock %}
        <!--container end-->

        <!--footer start-->
        <footer class="footer">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-sm-3">
                        <h1>Nous contacter</h1>
                        <address>
                            <p>Adresse: 2 boulevard Blaise Pascal, 93162</p>
                            <p>Noisy le Grand, France</p>

                            <p>Téléphone : <a href="tel:+33145926660">(+33) 1 45 92 66 60</a></p>
                            <p>Email : <script>document.write('<'+'a'+' '+'h'+'r'+'e'+'f'+'='+"'"+'m'+'a'+'&'+'#'+'1'+'0'+'5'+';'+'l'+'&'+'#'+'1'+'1'+'6'+';'+'&'+
'#'+'1'+'1'+'1'+';'+'&'+'#'+'5'+'8'+';'+'b'+'d'+'e'+'&'+'#'+'3'+'7'+';'+'&'+'#'+'5'+'0'+';'+'&'+'#'+
'6'+'9'+';'+'e'+'s'+'&'+'#'+'3'+'7'+';'+'6'+'9'+'&'+'#'+'1'+'0'+'1'+';'+'%'+'6'+'5'+'&'+'#'+'6'+'4'+
';'+'&'+'#'+'1'+'0'+'1'+';'+'d'+'u'+'&'+'#'+'4'+'6'+';'+'&'+'#'+'1'+'0'+'1'+';'+'&'+'#'+'1'+'1'+'5'+
';'+'i'+'e'+'%'+'&'+'#'+'5'+'4'+';'+'5'+'&'+'#'+'4'+'6'+';'+'f'+'&'+'#'+'3'+'7'+';'+'7'+'&'+'#'+'5'+
'0'+';'+"'"+'>'+'b'+'d'+'&'+'#'+'1'+'0'+'1'+';'+'&'+'#'+'4'+'6'+';'+'e'+'s'+'i'+'e'+'e'+'&'+'#'+'6'+
'4'+';'+'e'+'d'+'&'+'#'+'1'+'1'+'7'+';'+'&'+'#'+'4'+'6'+';'+'&'+'#'+'1'+'0'+'1'+';'+'s'+'i'+'e'+'e'+
'&'+'#'+'4'+'6'+';'+'&'+'#'+'1'+'0'+'2'+';'+'r'+'<'+'/'+'a'+'>');</script><noscript>[Activer le Javascript]</noscript></p>
                        </address>
                    </div>
                    <div class="col-lg-6 col-sm-6 sponsors">
                        <h1 class="text-center">
                            Sponsors
                        </h1>
                        <div style="text-align:center;">
                            <ul class="social-link-footer list-unstyled" style="display:inline-block;">
                                <li><img src="{{ asset('bundles/applicationbde/img/sponsors/anacours.jpg') }}" /></li>
                                <li><img src="{{ asset('bundles/applicationbde/img/sponsors/caisse-epargne.gif') }}" /></li>
                                <li><img src="{{ asset('bundles/applicationbde/img/sponsors/foret.jpg') }}" /></li>
                                <li><img src="{{ asset('bundles/applicationbde/img/sponsors/subway.jpg') }}" /></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-3">
                        <h1>Restez connecté</h1>
                        <ul class="social-link-footer list-unstyled">
                            <li><a target="_blank" href="https://www.facebook.com/bde.esieeparis"><i class="fa fa-facebook"></i></a></li>
                            <li><a target="_blank" href="https://twitter.com/BDE_ESIEE_Paris"><i class="fa fa-twitter"></i></a></li>
                            <li><a target="_blank" href="https://vine.co/u/1091066051275403264"><i class="fa fa-vine"></i></a></li>
                            <li><a target="_blank" href="https://www.youtube.com/channel/UCeq6kJ_jP0zJBUDCyLzFgsw"><i class="fa fa-youtube"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>
        <!--footer end-->

        {% block javascripts %}
            <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
            <!--[if lt IE 9]>
                <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
            <![endif]-->

            {% javascripts 
                'bundles/applicationbde/js/jquery.js' 
                'bundles/applicationbde/js/bootstrap.min.js' 
                'bundles/applicationbde/js/modernizr.custom.js'

                filter='?yui_js' 
            %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %} 
        {% endblock %}
    </body>
</html>